#!/bin/bash

xrandr --output VIRTUAL1 --off --output eDP1 --primary --mode 1920x1080 --pos 2560x360 --rotate normal --output DP1 --mode 2560x1440 --pos 4480x0 --rotate normal --output HDMI2 --mode 2560x1440 --pos 0x0 --rotate normal --output HDMI1 --off --output DP2 --off

feh --bg-scale ~/.config/i3/wallpaper.png

i3-msg workspace number 3
i3-msg move workspace to output DP1
i3-msg workspace number 2
i3-msg move workspace to output HDMI2
