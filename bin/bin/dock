#!/usr/bin/env bash

xrandr --output DP-2-1 --mode 2560x1440 --pos 0x0 --rotate normal --output DP-2-2 --off --output DP-2-3 --off --output eDP-1 --mode 1920x1080 --pos 2560x360 --rotate normal --output HDMI-2 --off --output HDMI-1 --off --output DP-2 --off --output DP-1 --off

feh --bg-scale ~/.config/i3/wallpaper.png

i3-msg workspace number 3
i3-msg move workspace to output DP-2-1
i3-msg workspace number 2
i3-msg move workspace to output DP-2-1
