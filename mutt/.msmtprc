account photocarl
host mail.photocarl.org
port 587
protocol smtp
auth on
from carlf@photocarl.org
user carlf
passwordeval gpg --no-tty -qd ~/.authinfo.gpg | grep mail.photocarl.org | awk '{print $6}'
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account opentable
host localhost
port 1025
protocol smtp
auth plain
from cflippin@opentable.com
user cflippin@opentable.com
passwordeval gpg --no-tty -qd ~/.authinfo.gpg | grep outlook.office365.com | awk '{print $6}'

account default : photocarl